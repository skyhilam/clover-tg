{
  "name": "clover/clover-tg",
  "description": "A clover tg",
  "license": "MIT",
  "keywords": [
    "clover",
    "tg"
  ],
  "type": "library",
  "authors": [
    {
      "name": "skyhilam",
      "email": "skyhilam@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^6.4",
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Clover\\CloverTg\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clover\\CloverTg\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clover\\CloverTg\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
